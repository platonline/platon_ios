# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'PlatonSDK' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for PlatonSDK
  pod 'Alamofire', '~> 5.2'
  pod 'SwiftHash', '~> 2.0.1'
  
end

target 'Sample' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    # Pods for PlatonSDK
    pod 'Alamofire', '~> 5.2'
    pod 'SwiftHash', '~> 2.0.1'
end
